{
  "name": "mirai-ts",
  "version": "0.5.0",
  "description": "Mirai TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "demo": "node demo/index.js",
    "prepublish": "npm run build",
    "build": "rm -rf dist && tsc",
    "docs:build": "typedoc --includeDeclarations --excludeExternals",
    "docs:dev": "cd docs && live-server",
    "lint": "eslint --fix . --ext .ts && prettier --write .",
    "watch": "rm -rf dist && tsc -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/mirai-ts"
  },
  "keywords": [
    "mirai",
    "http",
    "api",
    "sdk",
    "typescript",
    "node",
    "js"
  ],
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/ws": "^7.2.5",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "form-data": "^3.0.0",
    "ws": "^7.3.0"
  }
}
