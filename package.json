    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/ws": "^7.2.5",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "form-data": "^3.0.0",
    "ws": "^7.3.0"
  }
}
