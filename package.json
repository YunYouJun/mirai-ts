{
  "name": "mirai-ts",
  "version": "0.4.2",
  "description": "Mirai TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf dist && tsc",
