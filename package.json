{
  "name": "mirai-ts",
  "version": "2.1.1",
  "description": "Mirai TypeScript SDK",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "demo": "node demo/index.js",
    "demo:ts": "ts-node-dev demo/index.ts",
    "prepublishOnly": "npm run build",
    "build": "rimraf dist && rollup -c",
    "postbuild": "npm run build:type",
    "build:type": "api-extractor run",
    "postbuild:type": "rimraf \"dist/**/!(*.js?(.map)|index.d.ts)\"",
    "docs:build": "typedoc",
    "docs:dev": "cd docs && live-server",
    "lint": "eslint --fix . --ext .ts && prettier --write .",
    "mcl": "cd mcl && ./mcl",
    "watch": "rimraf dist && tsc -w",
    "test": "ts-node-dev test/index.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/mirai-ts"
  },
  "homepage": "https://www.yunyoujun.cn/mirai-ts/",
  "keywords": [
    "mirai",
    "http",
    "api",
    "sdk",
    "typescript",
    "node",
    "js"
  ],
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.19",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.11.10",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "husky": "7.0.4",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node-dev": "^1.1.8",
    "typedoc": "^0.22.10",
    "typescript": "~4.5.2"
  },
  "dependencies": {
    "@yunyoujun/logger": "^0.1.4",
    "axios": "^0.24.0",
    "chalk": "^5.0.0",
    "form-data": "^4.0.0",
    "ws": "^8.3.0"
  }
}
